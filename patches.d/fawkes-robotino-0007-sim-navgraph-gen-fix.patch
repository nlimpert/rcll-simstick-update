-  (not (sim-was-in-exploration))
-  (change-phase PRODUCTION)
-  ?sdt <- (sim-default-tag (name ?machine) (side ?side) (frame ?frame) (trans $?trans) (rot $?rot))
-  (not (default-navgraph-generated))
-  =>
-  (retract ?sdt)
